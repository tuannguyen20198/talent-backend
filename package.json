{
  "name": "talent-backend",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "aws:login": "aws sso login --profile nnpp-development",
    "build": "nest build",
    "build:sso": "nest build sso-service",
    "build:bff": "nest build bff-service",
    "build:talent": "nest build talent-service",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:dev:bff": "nest start --watch @apps/bff-service",
    "start:dev:sso": "dotenv -e apps/sso-service/.env -- nest start --watch @apps/sso-service",
    "start:dev:talent": "dotenv -e apps/talent-service/.env -- nest start --watch @apps/talent-service",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/apps/talent-backend/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./apps/talent-backend/test/jest-e2e.json",
    "prisma:generate-client:sso": "npx prisma generate --schema=apps/sso-service/prisma/schema.prisma",
    "prisma:generate:sso": "dotenv -e apps/sso-service/.env -- npx prisma migrate dev --schema=apps/sso-service/prisma/schema.prisma",
    "prisma:deploy:sso": "dotenv -e apps/sso-service/.env -- npx prisma migrate deploy --schema=apps/sso-service/prisma/schema.prisma",
    "prisma:push:sso": "dotenv -e apps/sso-service/.env -- npx prisma db push --schema=apps/sso-service/prisma/schema.prisma",
    "prisma:migrate:status:sso": "dotenv -e apps/sso-service/.env -- npx prisma migrate status --schema=apps/sso-service/prisma/schema.prisma",
    "prisma:migrate:squash:sso": "dotenv -e apps/sso-service/.env -- bash apps/sso-service/prisma/scripts/squash-migrations.sh --schema=apps/sso-service/prisma/schema.prisma",
    "prisma:generate-client:bff": "npx prisma generate --schema=apps/bff-service/prisma/schema.prisma",
    "prisma:generate:bff": "dotenv -e apps/bff-service/.env -- npx prisma migrate dev --schema=apps/bff-service/prisma/schema.prisma",
    "prisma:deploy:bff": "dotenv -e apps/bff-service/.env -- npx prisma migrate deploy --schema=apps/bff-service/prisma/schema.prisma",
    "prisma:migrate:status:bff": "dotenv -e apps/bff-service/.env -- npx prisma migrate status --schema=apps/bff-service/prisma/schema.prisma",
    "prisma:migrate:squash:bff": "dotenv -e apps/bff-service/.env -- bash apps/bff-service/prisma/scripts/squash-migrations.sh --schema=apps/bff-service/prisma/schema.prisma",
    "prisma:generate-client:talent": "npx prisma generate --schema=apps/talent-service/prisma/schema.prisma",
    "prisma:generate:talent": "dotenv -e apps/talent-service/.env -- npx prisma migrate dev --schema=apps/talent-service/prisma/schema.prisma",
    "prisma:push:talent": "dotenv -e apps/talent-service/.env -- npx prisma db push --schema=apps/talent-service/prisma/schema.prisma",
    "prisma:deploy:talent": "dotenv -e apps/talent-service/.env -- npx prisma migrate deploy --schema=apps/talent-service/prisma/schema.prisma",
    "prisma:migrate:status:talent": "dotenv -e apps/talent-service/.env -- npx prisma migrate status --schema=apps/talent-service/prisma/schema.prisma",
    "prisma:migrate:squash:talent": "dotenv -e apps/talent-service/.env -- bash apps/bff-service/prisma/scripts/squash-migrations.sh --schema=apps/talent-service/prisma/schema.prisma",
    "codegen": "openapi-generator-cli generate",
    "post-codegen": "pnpm lint && echo \"âœ… ESLint ran successfully!\""
  },
  "dependencies": {
    "@nestjs/axios": "^4.0.1",
    "@nestjs/common": "^11.0.1",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.0.1",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/platform-express": "^11.0.1",
    "@prisma/client": "^6.16.1",
    "axios": "^1.11.0",
    "class-transform": "^0.7.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "dotenv": "^17.2.2",
    "prisma": "^6.16.1",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.18.0",
    "@nestjs/cli": "^11.0.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.1",
    "@openapitools/openapi-generator-cli": "^2.23.4",
    "@swc/cli": "^0.6.0",
    "@swc/core": "^1.10.7",
    "@types/express": "^5.0.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^22.10.7",
    "@types/supertest": "^6.0.2",
    "dotenv-cli": "^10.0.0",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.2",
    "globals": "^16.0.0",
    "jest": "^29.7.0",
    "prettier": "^3.4.2",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.20.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/apps/",
      "<rootDir>/libs/"
    ],
    "moduleNameMapper": {
      "^@app/sso-client(|/.*)$": "<rootDir>/libs/sso-client/src/$1",
      "^@nnpp/talent-client(|/.*)$": "<rootDir>/libs/talent-client/src/$1",
      "^@tuan/common(|/.*)$": "<rootDir>/libs/common/src/$1",
      "^@tuan/constants(|/.*)$": "<rootDir>/libs/constants/src/$1",
      "^@tuan/contracts(|/.*)$": "<rootDir>/libs/contracts/src/$1",
      "^@tuan/postmark(|/.*)$": "<rootDir>/libs/postmark/src/$1"
    }
  }
}